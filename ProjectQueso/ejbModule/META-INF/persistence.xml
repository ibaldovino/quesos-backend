<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="ProjectQueso" transaction-type="JTA">
	<provider>org.hibernate.ejb.HibernatePersistence</provider>
		<jta-data-source>java:/OracleDS</jta-data-source>
	   <class>model.Camara</class>
		<class>model.Categoria</class>
		<class>model.Departamento</class>
		<class>model.Estusuario</class>
		<class>model.Evadescrip</class>
		<class>model.Evaluacione</class>
		<class>model.Evatipo</class>
		<class>model.Habijurado</class>
		<class>model.Habilidade</class>
		<class>model.Humedad</class>
		<class>model.Inscripcione</class>
		<class>model.Juracateg</class>
		<class>model.Jurado</class>
		<class>model.Localidade</class>
		<class>model.Maduracion</class>
		<class>model.Mesajura</class>
		<class>model.Mesa</class>
		<class>model.Pais</class>
		<class>model.Participante</class>
		<class>model.Queso</class>
		<class>model.Role</class>
		<class>model.Roltarea</class>
		<class>model.Subcatego</class>
		<class>model.Tarea</class>
		<class>model.Tecnologia</class>
		<class>model.Tipindustria</class>
		<class>model.Tipoleche</class>
		<class>model.Usuario</class>
		<class>model.Variedad</class>
		<properties>
		<property name="hibernate.dialect" value="org.hibernate.dialect.Oracle12cDialect"/>
		<property name="hibernate.hbm2ddl.auto" value="update"/>
		<property name="hibernate.show_sql" value="true"/>
		</properties>
		
	</persistence-unit>
</persistence>
